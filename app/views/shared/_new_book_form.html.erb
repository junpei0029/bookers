 <% books.each do |book| %>
    <tr>
      <td>
        <%= attachment_image_tag book.user, :profile_image, :fill, 40, 40, fallback: "no_image.jpg" %>
      </td>
      <td><%= link_to book.title, book_path(book.id) %> <%= book.title %></td>
      <td>
        book.body
      </td>
    </tr>
  <% end %>
  <%= paginate books %>